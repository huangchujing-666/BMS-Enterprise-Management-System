<script type="text/javascript">
    //关闭入库界面
    function ColseInsert() {
        $(".PopBg").hide();
        $("#UpdateBalance").hide();
        RefeshDown();
    }

    var updateScode;
    var updatevencode;
    function Subtract(scode, vencode) {

        updateScode = scode;
        updatevencode = vencode;
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/PageShowRk",
                data: {
                    "Scode": scode,
                    "Vencode": vencode,
                    "shopid": $("#ShopId").val(),
                },
                success: function (data) {
                    var da = data.split('❤')
                    $("#tdScode").html(da[0]);//货号
                    $("#tdVenName").html(da[1]);//数据源名称
                    $("#lbCountBalance").html(da[2]);//总库存
                    $("#TableShopBalance").html(da[3]);//写入表
                    $("#AllCounts").html(da[4]);//总记录数
                    $("#Count").html(da[5]);//总页数
                    $("#SurplusBalance").html(da[6]);//剩余库存
                    $(".PopBg").height($(document).height());
                    $(".PopBg").show();
                    $("#UpdateBalance").show();
                }
            })

    }
    ///确认修改库存
    function SaveUpdateBalance() {
        var balance = $("#BalanceSum").val();
        if (balance < 0) {
            alert("库存不允许为负数");
            return false;
        }
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/UpdateBalanceByAndShopIdVencode",
                data: {
                    "Scode": updateScode,
                    "Vencod": updatevencode,
                    "Balance": balance,
                    "shopid": $("#ShopId").val(),
                },
                success: function (data) {
                    alert(data);
                    Subtract(updateScode, updatevencode);

                }
            })
    }


    /*关闭按钮取消修改*/
    function Close() {
        $(".PopBg").hide();
        $(".myPopdiv").hide();
    }
</script>
