<script type="text/javascript">
    $(function () {
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/WarehouseShopShow",
                success: function (data) {
                    var da = data.split('❤');
                    $("#Condition").html(da[0]);
                    $("#ConditionDown").html(da[1]);
                }
            })
    })
    ///全选
    function CheckAll() {
        if ($("#Checkall").attr("checked") == "checked") {
            $("input[id=Choose]").each(function () {
                $(this).attr("checked", true);
            })
        }
        else {
            $("input[id=Choose]").each(function () {
                $(this).attr("checked", false);
            })
        }
    }


    //款号查询
    function Search() {
        $("#PageReturn").val("1");
        PageIndexSearch(2);
    }
    //款号分页
    function PageIndexSearch(index) {
        var style = $("#StyleTop").val() == null ? "" : $("#StyleTop").val();
        var priceMin = $("#priceMin").val() == null ? "" : $("#priceMin").val();
        var priceMax = $("#priceMax").val() == null ? "" : $("#priceMax").val();
        var Cat = $("#Cat").val() == null ? "" : $("#Cat").val();
        var BalanceMin = $("#BalanceMin").val() == null ? "" : $("#BalanceMin").val();
        var BalanceMax = $("#BalanceMax").val() == null ? "" : $("#BalanceMax").val();
        var Cat2 = $("#Cat2").val() == null ? "" : $("#Cat2").val();
        var Imagefile = $("#Imagefile").val() == null ? "" : $("#Imagefile").val();
        var salesState = $("#salesState").val() == null ? "" : $("#salesState").val();
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/SearchPage",
                data: {
                    "style": style,
                    "priceMin": priceMin,
                    "priceMax": priceMax,
                    "Cat": Cat,
                    "BalanceMin": BalanceMin,
                    "BalanceMax": BalanceMax,
                    "Cat2": Cat2,
                    "Imagefile": Imagefile,
                    "salesState": salesState,
                    "Index": index,
                    "PageReturn": $("#PageReturn").val() == null ? "" : $("#PageReturn").val(),
                    "shopid": $("#ShopId").val(),
                    "Page": $("#PageTop").val() == "" ? 10 : $("#PageTop").val()
                },
                success: function (data) {
                    var da = data.split('❤');
                    $("#TopTable").empty;
                    $("#AllCounts").empty;
                    $("#PageCount").empty;
                    $("PageNum").empty;
                    $("#TopTable").html(da[0]);
                    $("#AllCounts").html(da[1]);
                    $("#PageCount").html(da[2]);
                    $("#PageNum").html(da[3]);
                    $("#PageReturn").val(da[3]);
                    showPic1();
                }
            })
    }
    //刷新款号
    function Refesh() {
        PageIndexSearch(2);
    }
    //页码更改  款号
    function StyleChage()
    {
        $("#PageReturn").val("1");
        PageIndexSearch(2);
    }

    //货号查询
    function SearchDown() {
        $("#PageT").val("1");
        PageIndexScode(2)
    }
    //货号分页
    function PageIndexScode(index) {
        var style = $("#Style").val() == null ? "" : $("#Style").val();
        var scode = $("#ScodeDown").val() == null ? "" : $("#ScodeDown").val();
        var priceMin = $("#PriceMinDown").val() == null ? "" : $("#PriceMinDown").val();
        var priceMax = $("#PriceMaxDown").val() == null ? "" : $("#PriceMaxDown").val();
        var Cat = $("#CatDown").val() == null ? "" : $("#CatDown").val();
        var BalanceMin = $("#StcokMinDown").val() == null ? "" : $("#StcokMinDown").val();
        var BalanceMax = $("#StcokMaxDown").val() == null ? "" : $("#StcokMaxDown").val();
        var Cat2 = $("#Cat2Down").val() == null ? "" : $("#Cat2Down").val();
        var minTime = $("#MinTimeDown").val() == null ? "" : $("#MinTimeDown").val();
        var maxTime = $("#MaxTimeDown").val() == null ? "" : $("#MaxTimeDown").val();
        var salesState = $("#salesStatedown").val() == null ? "" : $("#salesStatedown").val();
        var Imagefiledown = $("#Imagefiledown").val() == null ? "" : $("#Imagefiledown").val();
        $.ajax(
            {
                type: "POST",
                url: "/ShopManager/WarehouseShopShowDownPage",
                datatype: "text",
                data:
                    {
                        "style": style,
                        "scode": scode,
                        "priceMin": priceMin,
                        "priceMax": priceMax,
                        "Cat": Cat,
                        "BalanceMin": BalanceMin,
                        "BalanceMax": BalanceMax,
                        "Cat2": Cat2,
                        "minTime": minTime,
                        "maxTime": maxTime,
                        "Index": index,
                        "PageReturnD": $("#PageT").val() == "" ? 0 : $("#PageT").val(),
                        "salesState": salesState,
                        "Imagefiledown": Imagefiledown,
                        "shopid": $("#ShopId").val(),
                        "Page": $("#PageDown").val() == "" ? 10 : $("#PageDown").val()
                    },
                success: function (data) {
                    var da = data.split('❤');
                    $("#downTable").empty;
                    $("#PageCountD").empty;
                    $("#AllCountsD").empty;
                    $("#PageNumD").empty;
                    $("#downTable").html(da[0]);
                    $("#PageCountD").html(da[2]);
                    $("#PageNumD").html(da[3]);
                    $("#PageT").val(da[3]);
                    $("#AllCountsD").html(da[1]);
                    showPic();
                }

            })
    }
    //刷新  货号
    function RefeshDown() {
        PageIndexScode(2);
    }
    //页码更改 货号
    function ScodeChage() {
        $("#PageT").val("1");
        PageIndexScode(2)
    }


    //查看图片
    function LookAttr(style) {
        $.get("/ShopManager/ShowMainPicStyle", { style: style }, function (data) {
            var da = data.split('-*-');
            $("#divMainPic").html("");
            for (var i = 0; i < da.length; i++) {
                $("#divMainPic").append("<img style='width:75px;height:75px;' src='" + da[i] + "'/>")
            }
        })//主图
        //$.get("/ShopManager/ShowPeiPic", { style: style }, function (data) { $("#divBrandPeiPic").empty; $("#divBrandPeiPic").append(data) })//配图
        //$.get("/ShopManager/ShowDetailPic", { style: style }, function (data) { $("#divProductDetail").empty; $("#divProductDetail").append(data) })//细节图
        $("#PopStyledescript").show();
    }
    //开放销售
    function OpenSale() {
        var styles = "";
        $("input[id=Choose]").each(function () {
            if ($(this).attr("checked") == "checked") {
                styles += $(this).attr("styleName") + "❤";
            }
        })
        var count = styles.split('❤').length;
        if (count > 1) {
            if (confirm("是否确认开放销售？")) {
                $.ajax(
                    {
                        type: "POST",
                        datatype: "text",
                        url: "/ShopManager/OpenSale",
                        data: {
                            "styles": styles,
                            "shopid": $("#ShopId").val(),
                        },
                        success: function (data) {
                            alert(data);
                            Refesh()
                        }
                    })
            }
        }
        else {
            alert("请选择需要开放销售的款号！");
        }
    }
    //取消销售
    function CancelSale() {
        var styles = "";
        $("input[id=Choose]").each(function () {
            if ($(this).attr("checked") == "checked") {
                styles += $(this).attr("styleName") + "❤";
            }
        })
        var count = styles.split('❤').length;
        if (count > 1) {
            if (confirm("是否确认取消销售？")) {
                $.ajax(
                    {
                        type: "POST",
                        datatype: "text",
                        url: "/ShopManager/CancelSale",
                        data: {
                            "styles": styles,
                            "shopid": $("#ShopId").val(),
                        },
                        success: function (data) {
                            alert(data);
                            Refesh()
                        }
                    })
            }
        }
        else {
            alert("请选择需要取消销售的款号！");
        }
    }
    ///删除
    function Delete(style) {
        if (confirm("确认 " + style + " 是否删除？")) {
            $.ajax(
                {
                    type: "POST",
                    datatype: "text",
                    url: "/ShopManager/DeleteByStyleAndShopid",
                    data: {
                        "styles": style,
                        "shopid": $("#ShopId").val(),
                    },
                    success: function (data) {
                        alert(data);
                        Refesh()
                    }
                })
        }
    }
    //批量删除
    function DeleteAll() {
        var styles = "";
        $("input[id=Choose]").each(function () {
            if ($(this).attr("checked") == "checked") {
                styles += $(this).attr("styleName") + "❤";
            }
        })
        var count = styles.split('❤').length;
        if (count > 1) {
            if (confirm("是否确认批量删除？")) {
                $.ajax(
                    {
                        type: "POST",
                        datatype: "text",
                        url: "/ShopManager/DeleteByStyleAndShopidBatch",
                        data: {
                            "styles": styles,
                            "shopid": $("#ShopId").val(),
                        },
                        success: function (data) {
                            alert(data);
                            Refesh()
                        }
                    })
            }
        }
        else {
            alert("请选择需要删除的款号！");
        }
    }
    ///按照货号删除
    function DeleteScode(scode) {
        if (confirm("确认 " + scode + " 是否删除？")) {
            $.ajax(
                {
                    type: "POST",
                    datatype: "text",
                    url: "/ShopManager/DeleteScode",
                    data: {
                        "Scode": scode,
                        "shopid": $("#ShopId").val(),
                    },
                    success: function (data) {
                        alert(data);
                        RefeshDown()
                    }
                })
        }
    }

    ///打开修改价格的页面
    var Pricestyle;
    var PriceScode;
    function UpdatePricea(style, scode) {
        Pricestyle = style;
        PriceScode = scode;
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/GetPriceByScodeandShopid",
                data:
                    {
                        "Scode": scode,
                        "shopid": $("#ShopId").val(),
                    },
                success: function (data) {
                    var da = data.split('❤');
                    $("#PirceScode").html(scode);
                    $("#Pricea").html(da[0]);
                    $("#Priceb").html(da[1]);
                    $("#Pricec").html(da[2]);
                    $("#Priced").html(da[3]);
                    $("#Pricee").html(da[4]);

                }
            })
        $("#UpdatePrice").show();
        $(".PopBg").height($(document).height());
        $(".PopBg").show();
    }
    ///关闭修改界面
    function ColseUpdateBalance() {
        $("#UpdatePrice").hide();
    }
    ///确认修改价格
    function SaveUpdatePrice() {
        if (confirm("确认是否修改款号 " + Pricestyle + " 售货价")) {
            var pricec = $("#Pricee").html();
            var UpatePricec = $("#UpatePricec").val();
            if (UpatePricec == "") {
                alert("修改的价格不能为空！")
                return false;
            }
            if (UpatePricec < 0) {
                alert("价格不允许为负数！");
                return false;
            }
            $.ajax(
                {
                    type: "POST",
                    datatype: "text",
                    url: "/ShopManager/UpdatePricecByStyleAndPrice",
                    data: {
                        "style": Pricestyle,
                        "pricec": pricec,
                        "UpatePricec": UpatePricec,
                        "scode": PriceScode,
                        "shopid": $("#ShopId").val(),
                    },
                    success: function (data) {
                        alert(data);
                        $("#UpdatePrice").hide();
                        $(".PopBg").hide();
                        RefeshDown();
                    }
                })
        }

    }
    //查看属性
    function LookAtrrbutt(scode, vencode) {
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/ShopManager/GetShopProductByScodeAndShopIdVencode",
                data: {
                    "scode": scode,
                    "vencode": vencode,
                    "shopid": $("#ShopId").val(),
                },
                success: function (data) {
                    var da = data.split('❤');
                    $("#EditContent").empty;
                    $("#divProductInfo").empty;
                    $("#TypeAttribute").empty;
                    $.get("/ShopManager/ShowMainPic", { scode: scode }, function (data) {
                        var da = data.split('-*-');
                        $("#divShowMianPic").html("");
                        for (var i = 0; i < da.length - 1; i++) {
                            $("#divShowMianPic").append("<img style='width:75px;height:75px;' src='" + da[i] + "'/>")
                        }
                        MaxPic();

                    })//主图

                    $("#EditContent").html(da[0]);
                    //$("#divProductInfo").html(da[1]);
                    $("#TypeAttribute").html(da[2]);
                    $("#PopContent").show();
                    $(".PopBg").height($(document).height());
                    $(".PopBg").show();
                    

                }
            })
    }
    //关闭查看属性
    function CloseDown() {
        $("#PopContent").hide();
    }
    //查看商品信息
    function LookShop(style) {
        var style = style;
        $("#Style").val(style)
        $("#PageT").val("1");
        PageIndexScode(2)
    }
</script>
