<script type="text/ecmascript">
    ///打开修改价格的页面
    var Pricestyle;
    var PriceScode;
    var priceVencode
    function UpdatePricea(style, scode, vencode) {
        Pricestyle = style;
        PriceScode = scode;
        priceVencode = vencode;
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/SoCalProduct/GetPriceByScodeAndShopId",
                data:
                    {
                        "Scode": scode,
                        "Vencode": priceVencode,
                        "ShopId": $("#title").attr("title"),
                    },
                success: function (data) {
                    var da = data.split('❤');
                    $("#PirceScode").html(scode);
                    $("#Pricea").html(da[0]);
                    $("#Priceb").html(da[1]);
                    $("#Pricec").html(da[2]);
                    $("#Priced").html(da[3]);
                    $("#Pricee").html(da[4]);

                }
            })
        $("#UpdatePrice").show();
        $(".PopBg").height($(document).height());
        $(".PopBg").show();
    }
    ///关闭修改界面
    function ColseUpdateBalance() {
        $("#UpdatePrice").hide();
    }
    ///确认修改价格
    function SaveUpdatePrice() {
        if (confirm("确认是否修改款号 " + Pricestyle + " 售货价")) {
            var pricec = $("#Pricee").html();
            var UpatePricec = $("#UpatePricec").val();
            if (UpatePricec < 0) {
                alert("价格不允许为负数！");
                return false;
            }
            $.ajax(
                {
                    type: "POST",
                    datatype: "text",
                    url: "/SoCalProduct/UpdateSoCalPriceByStyleAll",
                    data: {
                        "style": Pricestyle,
                        "pricec": pricec,
                        "UpatePricec": UpatePricec,
                        "scode": PriceScode,
                        "Vencode": priceVencode,
                        "ShopId": $("#title").attr("title"),
                    },
                    success: function (data) {
                        alert(data);
                        $("#UpdatePrice").hide();
                        $(".PopBg").hide();
                        ScCalSearchScode();

                    }
                })
        }

    }
</script>
