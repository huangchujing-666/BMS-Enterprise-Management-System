<script type="text/javascript">
    //页面加载时加载根据权限 加载查询条件
    $(function () {
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/SoCalProduct/SoCalWareHouseShow",
                data:{"MenuId":"@ViewData["MenuId"]"},
                success: function (data) {
                    var da = data.split('❤');
                    $("#Condition").html(da[0]);
                    $("#ConditionDown").html(da[1]);
                }
            })
    })
    //查询-款号
    function SoCalSearchStyle() {
        SoCalSearchPage(0);
    }
    //分页-款号
    function SoCalSearchPage(Index) {
        var style = $("#StyleTop").val() == null ? "" : $("#StyleTop").val();
        var priceMin = $("#priceMin").val() == null ? "" : $("#priceMin").val();
        var priceMax = $("#priceMax").val() == null ? "" : $("#priceMax").val();
        var Cat = $("#Cat").val() == null ? "" : $("#Cat").val();
        var BalanceMin = $("#BalanceMin").val() == null ? "" : $("#BalanceMin").val();
        var BalanceMax = $("#BalanceMax").val() == null ? "" : $("#BalanceMax").val();
        var Cat2 = $("#Cat2").val() == null ? "" : $("#Cat2").val();
        var Imagefile = $("#Imagefile").val() == null ? "" : $("#Imagefile").val();
        var salesState = $("#salesState").val() == null ? "" : $("#salesState").val();
        $.ajax(
            {
                type: "POST",
                datatype: "text",
                url: "/SoCalProduct/SoCalSearch",
                data: {
                    "style": style,
                    "priceMin": priceMin,
                    "priceMax": priceMax,
                    "Cat": Cat,
                    "BalanceMin": BalanceMin,
                    "BalanceMax": BalanceMax,
                    "Cat2": Cat2,
                    "Imagefile": Imagefile,
                    "salesState": salesState,
                    "IsPage": true,
                    "Index": Index,
                    "ThisPage": $("#Pagetop").val() == "" ? 1 : $("#Pagetop").val(),
                    "Page": $("#Page").val() == "" ? 10 : $("#Page").val(),
                    "ShopId": $("#title").attr("title"),
                },
                success: function (data) {
                    var da = data.split('❤');
                    $("#TableTop").empty;
                    $("#AllCountstop").empty;
                    $("#PageCounttop").empty;
                    $("#PageNumtop").empty;
                    $("#TableTop").html(da[0]);
                    $("#AllCountstop").html(da[1]);
                    $("#PageCounttop").html(da[2]);
                    $("#PageNumtop").html(da[3]);
                    $("#Pagetop").val(da[3])
                }
            })
    }
    //页码改变
    function ChangePageCount() {
        SoCalSearchPage(2);
    }
    //查询-货号
    function ScCalSearchScode() {
        ScCalSearchScodePage(2)
    }
    //分页-货号
    function ScCalSearchScodePage(Index) {
        var style = $("#Style").val() == null ? "" : $("#Style").val();
        var scode = $("#ScodeDown").val() == null ? "" : $("#ScodeDown").val();
        var priceMin = $("#PriceMinDown").val() == null ? "" : $("#PriceMinDown").val();
        var priceMax = $("#PriceMaxDown").val() == null ? "" : $("#PriceMaxDown").val();
        var Cat = $("#CatDown").val() == null ? "" : $("#CatDown").val();
        var BalanceMin = $("#StcokMinDown").val() == null ? "" : $("#StcokMinDown").val();
        var BalanceMax = $("#StcokMaxDown").val() == null ? "" : $("#StcokMaxDown").val();
        var Cat2 = $("#Cat2Down").val() == null ? "" : $("#Cat2Down").val();
        var minTime = $("#MinTimeDown").val() == null ? "" : $("#MinTimeDown").val();
        var maxTime = $("#MaxTimeDown").val() == null ? "" : $("#MaxTimeDown").val();
        var salesState = $("#salesStatedown").val() == null ? "" : $("#salesStatedown").val();
        var Imagefiledown = $("#Imagefiledown").val() == null ? "" : $("#Imagefiledown").val();
        $.ajax(
            {
                type: "POST",
                url: "/SoCalProduct/ScCalSearchScode",
                datatype: "text",
                data:
                    {
                        "style": style,
                        "scode": scode,
                        "priceMin": priceMin,
                        "priceMax": priceMax,
                        "Cat": Cat,
                        "BalanceMin": BalanceMin,
                        "BalanceMax": BalanceMax,
                        "Cat2": Cat2,
                        "minTime": minTime,
                        "maxTime": maxTime,
                        "salesState": salesState,
                        "Imagefiledown": Imagefiledown,
                        "IsPage": true,
                        "Index": Index,
                        "ThisPage": $("#PageD").val() == "" ? 1 : $("#PageD").val(),
                        "Page": $("#PageDown").val() == "" ? 10 : $("#PageDown").val(),
                        "ShopId": $("#title").attr("title"),
                    },
                success: function (data) {
                    var da = data.split('❤');
                    $("#TableDown").empty;
                    $("#PageCountD").empty;
                    $("#AllCountsD").empty;
                    $("#PageNumD").empty;
                    $("#TableDown").html(da[0]);
                    $("#PageCountD").html(da[2]);
                    $("#AllCountsD").html(da[1]);
                    $("#PageNumD").html(da[3]);
                    $("#PageD").val(da[3])
                }

            })
    }
    //页码改变
    function ChangePageCountDown()
    {
        ScCalSearchScodePage(2)
    }
    //查看商品
    function LookShop(style) {
        $("#Style").val(style);
        ScCalSearchScode();

    }
</script>
