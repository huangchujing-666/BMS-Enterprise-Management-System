<script type="text/javascript">

    ///全选
    function CheckAll() {
        if ($("#Checkall").attr("checked") == "checked") {
            $("input[id=Choose]").each(function () {
                $(this).attr("checked", true);
            })
        }
        else {
            $("input[id=Choose]").each(function () {
                $(this).attr("checked", false);
            })
        }
    }
    //开放销售
    function OpenSale() {
        var styles = "";
        $("input[id=Choose]").each(function () {
            if ($(this).attr("checked") == "checked") {
                styles += $(this).attr("styleName") + "❤";
            }
        })
        var count = styles.split('❤').length;
        if (count > 1) {
            if (confirm("是否确认开放销售？")) {
                $.ajax(
                    {
                        type: "POST",
                        datatype: "text",
                        url: "/SoCalProduct/OpenSale",
                        data: {
                            "styles": styles,
                            "ShopId": $("#title").attr("title"),
                        },
                        success: function (data) {
                            alert(data);
                            SoCalSearchStyle();
                        }
                    })
            }
        }
        else {
            alert("请选择需要开放销售的款号！");
        }
    }
    //取消销售
    function CancelSale() {
        var styles = "";
        $("input[id=Choose]").each(function () {
            if ($(this).attr("checked") == "checked") {
                styles += $(this).attr("styleName") + "❤";
            }
        })
        var count = styles.split('❤').length;
        if (count > 1) {
            if (confirm("是否确认取消销售？")) {
                $.ajax(
                    {
                        type: "POST",
                        datatype: "text",
                        url: "/SoCalProduct/CancelSale",
                        data: {
                            "styles": styles,
                            "ShopId": $("#title").attr("title"),
                        },
                        success: function (data) {
                            alert(data);
                            SoCalSearchStyle();
                        }
                    })
            }
        }
        else {
            alert("请选择需要取消销售的款号！");
        }
    }
</script>
